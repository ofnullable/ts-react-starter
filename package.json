{
  "name": "ts-react-starter",
  "version": "0.2.4",
  "description": "React Typescript start",
  "main": "src/server.ts",
  "scripts": {
    "start": "nodemon",
    "prestart": "npm run build:server && npm run build:client",
    "start:prod": "npm run build:prod && NODE_ENV=production node ./dist/server",
    "build:client": "NODE_ENV=development webpack --config config/webpack.client.config.js",
    "build:server": "NODE_ENV=development webpack --config config/webpack.server.config.js",
    "build:prod": "npm run build:prod-server && npm run build:prod-client",
    "build:prod-client": "NODE_ENV=production webpack --mode production --config config/webpack.client.config.js",
    "build:prod-server": "NODE_ENV=production webpack --mode production --config config/webpack.server.config.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ofnullable/ts-react-starter.git"
  },
  "author": "ofnullable <ofnullable@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ofnullable/ts-react-starter/issues"
  },
  "homepage": "https://github.com/ofnullable/ts-react-starter#readme",
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.35",
    "@loadable/component": "^5.14.1",
    "@loadable/server": "^5.14.0",
    "axios": "^0.21.0",
    "express": "^4.17.1",
    "immer": "^7.0.14",
    "react": "^17.0.1",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.14.0",
    "@types/express": "^4.17.8",
    "@types/loadable__component": "^5.13.1",
    "@types/loadable__server": "^5.12.3",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.6",
    "@types/webpack-dev-middleware": "^3.7.2",
    "@types/webpack-env": "^1.15.3",
    "@types/webpack-hot-middleware": "^2.25.3",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^10.0.0",
    "css-loader": "^5.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.2.1",
