{
  "name": "ts-react-starter",
  "version": "0.3.0",
  "description": "React Typescript start",
  "main": "src/server",
  "scripts": {
    "start": "nodemon",
    "prestart": "npm run build:server && npm run build:client",
    "build:client": "NODE_ENV=development webpack --config config/webpack.client.config.js",
    "build:server": "NODE_ENV=development webpack --config config/webpack.server.config.js",
    "start:prod": "npm run build:prod && NODE_ENV=production node ./dist/server",
    "build:prod": "npm run build:prod-server && npm run build:prod-client",
    "build:prod-client": "NODE_ENV=production webpack --mode production --config config/webpack.client.config.js",
    "build:prod-server": "NODE_ENV=production webpack --mode production --config config/webpack.server.config.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ofnullable/ts-react-starter.git"
  },
  "author": "ofnullable <ofnullable@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ofnullable/ts-react-starter/issues"
  },
  "homepage": "https://github.com/ofnullable/ts-react-starter#readme",
  "dependencies": {
    "@babel/runtime": "^7.12.18",
    "@emotion/react": "^11.1.5",
    "@loadable/component": "^5.14.1",
    "@loadable/server": "^5.14.2",
    "axios": "^0.21.0",
    "immer": "^8.0.1",
    "koa": "^2.13.1",
    "koa-static": "^5.0.0",
    "react": "^17.0.1",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/plugin-transform-runtime": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
    "@emotion/babel-plugin": "^11.2.0",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.14.2",
    "@types/koa": "^2.13.0",
    "@types/koa-static": "^4.0.1",
    "@types/loadable__component": "^5.13.3",
    "@types/loadable__server": "^5.12.3",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.6",
    "@types/webpack-dev-middleware": "^4.1.0",
    "@types/webpack-env": "^1.15.3",
    "@types/webpack-hot-middleware": "^2.25.3",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.2",
    "cssnano": "^4.1.10",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.3.8",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.6",
    "postcss": "^8.2.6",
    "postcss-flexbugs-fixes": "^5.0.0",
    "postcss-loader": "^5.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^2.5.2"
  }
}
